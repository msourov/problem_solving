   "source": [
    "# Missing Number\n",
    "\n",
    "def missing_number(n, arr):\n",
    "    s = sum(arr)\n",
    "    return int(n*(n+1)/2-s) # n*(n+1)/2 produces sum of all positive integers till n\n",
    "\n",
    "n = int(input())\n",
    "arr = list(map(int, input().split(' ')))\n",
    "print(missing_number(n, arr))"
   ]
  }
