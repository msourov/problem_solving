    "\n",
    "def weird(n):\n",
    "    while True:\n",
    "        print(f'{n} ',end='')\n",
    "        if n == 1:\n",
    "            return\n",
    "        if n%2 == 0:\n",
    "            n = n//2\n",
    "        else:\n",
    "            n = n*3+1\n",
    "        \n",
    "weird(int(input()))"
